<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include layout="@layout/activity_search_titlebar"/>
    <include layout="@layout/line_linearlayout"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <include layout="@layout/activity_search_first_view"/>
            <include layout="@layout/line_linearlayout"/>
            <include layout="@layout/activity_search_second_view"/>
            <include layout="@layout/line_linearlayout"/>
            <include layout="@layout/activity_search_third_view"/>


        </LinearLayout>
    </ScrollView>



</LinearLayout>
